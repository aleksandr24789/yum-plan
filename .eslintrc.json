{
"env": {
    "browser": true,
    "es2021": true
},
"extends": [
    "eslint:recommended",
    "next",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:import/recommended",
    "plugin:react/jsx-runtime",
    "prettier"
],
"overrides": [
    {
        "env": {
            "node": true
        },
        "files": [
            ".eslintrc.{js,cjs}"
        ],
        "parserOptions": {
            "sourceType": "script"
        }
    }
],
"parserOptions": {
    "ecmaVersion": "latest",
    "ecmaFeatures": {
        "jsx": true
    },
    "sourceType": "module"
},
"plugins": [
    "react"
],
"rules": {
    "react/jsx-filename-extension": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off"
}
}